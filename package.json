{
  "name": "express-api-boilerplate",
  "version": "0.0.0",
  "git": "https://github.com/artegha-digital/express-api-boilerplate.git",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specidied\" && exit 1",
    "start": "npm run dev",
    "dev": "set NODE_ENV=development && nodemon --exec babel-node -r dotenv/config ./src/bin/www",
    "prod": "set NODE_ENV=production npm-run-all clean build && node ./dist/bin/www",
    "clean": "shx rm -r dist",
    "build": "babel ./src -d dist"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.21.6",
    "tedious": "^8.2.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-plugin-root-import": "^6.5.0",
    "nodemon": "^2.0.3",
    "shx": "^0.3.2"
  }
}
